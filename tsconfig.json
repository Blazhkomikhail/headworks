{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "noEmitOnError": true,
    "noUnusedLocals": false,
    "removeComments": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "CommonJS",
    "allowJs": true,
    "sourceMap": true,
  },
  "files": [
    "./declarations.d.ts"
  ],
  "include": [
    "./src/Client.tsx",
  ],
  "exclude": [
    "node_modules"
  ]
}